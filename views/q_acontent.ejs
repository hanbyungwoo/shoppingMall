	<h1><%= title %></h1>
<form  action="/Q&A/q_acontent" method="post" onsubmit ="return onWriteSubmit()">

		
		<input type="hidden" name="contentnum" value="<%=row.contentnum%>"/>
		<tr>
		
			<tr>
			<td>패스워드</td>
			<td><input type= "password" name="passwd" id= "passwd" required/></td>
			</tr>

			<tr>
